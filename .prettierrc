{
    "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
    "semi": false,
    "tabWidth": 4,
    "printWidth": 80,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ]
}
